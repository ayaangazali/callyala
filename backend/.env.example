# Voice Agent Ops - MVP Backend Environment Variables# Environment Configuration

# Copy this file to .env and fill in the values

# App

APP_ENV=local# Database

PORT=8000DATABASE_URL=postgresql+asyncpg://postgres:postgres@localhost:5432/voice_agent_ops

DATA_DIR=./data

# Redis (for background jobs and pub/sub)

# Google SheetsREDIS_URL=redis://localhost:6379/0

GOOGLE_SHEETS_SPREADSHEET_ID=your-spreadsheet-id-here

