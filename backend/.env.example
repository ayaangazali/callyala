# Environment Configuration
# Copy this file to .env and fill in the values

# Database
DATABASE_URL=postgresql+asyncpg://postgres:postgres@localhost:5432/voice_agent_ops

# Redis (for background jobs and pub/sub)
REDIS_URL=redis://localhost:6379/0

# Security
JWT_SECRET=your-super-secret-key-change-in-production
JWT_ALGORITHM=HS256
JWT_EXPIRE_MINUTES=1440

# ElevenLabs Integration
ELEVENLABS_API_KEY=your-elevenlabs-api-key
ELEVENLABS_WEBHOOK_SECRET=your-webhook-secret-from-elevenlabs

# CORS (comma-separated origins)
CORS_ORIGINS=http://localhost:8080,http://localhost:3000

# App Settings
APP_ENV=development
DEBUG=true
LOG_LEVEL=INFO

# Optional: Default org for seeding
DEFAULT_ORG_ID=
