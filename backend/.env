# Environment Configuration
# Copy this file to .env and fill in the values

# Database (SQLite for development, replace with PostgreSQL for production)
# For now using a local dev database - replace with your PostgreSQL URL
DATABASE_URL=postgresql+asyncpg://postgres:postgres@localhost:5432/voice_agent_ops

# Redis (for background jobs and pub/sub)
REDIS_URL=redis://localhost:6379/0

# Security - CHANGE THESE IN PRODUCTION!
JWT_SECRET=dev-secret-key-change-me-in-production-12345678
JWT_ALGORITHM=HS256
JWT_EXPIRE_MINUTES=1440

# ElevenLabs Integration - Add your keys here
ELEVENLABS_API_KEY=your-elevenlabs-api-key-here
ELEVENLABS_WEBHOOK_SECRET=your-webhook-secret-from-elevenlabs-here

# CORS (comma-separated origins)
CORS_ORIGINS=http://localhost:5173,http://localhost:3000,http://localhost:8080

# App Settings
APP_ENV=development
DEBUG=true
LOG_LEVEL=INFO

# Optional: Default org for seeding
DEFAULT_ORG_ID=
